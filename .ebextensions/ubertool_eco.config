commands:
  yum_updates: 
    command: "yum update -y"
  pip_install: 
    command: "yum install python-pip -y"
  numpy_install:
    command: "pip install numpy"

option_settings:
  - namespace: aws:elasticbeanstalk:container:python
    option_name: WSGIPath
    value: wsgi.py
  - option_name: DJANGO_SETTINGS_MODULE
    value: settings